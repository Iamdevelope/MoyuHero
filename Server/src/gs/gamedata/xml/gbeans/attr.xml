			  <variable name="huoli" fromCol="活力上限" type="int"  /> 
			  <variable name="invitetimes" fromCol="邀请战次数" type="int"  />
			  <variable name="skillexp1" fromCol="计策升级经验白" type="int"  /> 
			  <variable name="skillexp2" fromCol="计策升级经验绿" type="int"  /> 
			  <variable name="skillexp3" fromCol="计策升级经验蓝" type="int"  /> 
