		<variable name="key" type="int" validator="value=[1,)"/>key
		<variable name="timer" type="short"/>每天使用次数
		<variable name="number" type="short" validator="value=[1,)"/>数量
		<variable name="extdata" type="octets"/> 额外数据
	</bean>
	<bean name="Bag">
		<variable name="items" type="vector" value="Item"/>	
	</bean>
	
