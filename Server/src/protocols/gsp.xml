	</protocol>
	
	<bean name="RoleInfo">
		<variable name="roleid" type="long" />		ID
		<variable name="rolename" type="string"/>	名称
		<variable name="rolelevel" type="short"/>	等级
	</bean>
	
	<!-- 服务器发给客户端，已有角色信息列表 -->
	<protocol  name="SRoleList" type="2" maxsize="4096" prior="1">
		<variable name="role" type="list" value="RoleInfo" />
	</protocol>
	
		
	<!-- 客户端请求创建 -->
	<protocol  name="CCreateRole" type="3" maxsize="4096" prior="1"  waiting="4">
		<variable name="name" type="string" />
		<variable name="firsthero" type="int" />
