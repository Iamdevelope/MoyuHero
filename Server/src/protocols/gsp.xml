	<protocol  name="SCreateRole" type="4" maxsize="65535" prior="1"> 
		<enum name="CREATE_OK" value="1"/>  成功		     
		<enum name="CREATE_ERROR" value="2"/>    失败 
		<enum name="CREATE_INVALID" value="3"/> 名称不合法 		
		<enum name="CREATE_DUPLICATED" value="4"/>重名 		
		<enum name="CREATE_OVERCOUNT" value="5"/> 创建的新角色数量过多
		<enum name="CREATE_OVERLEN" value="6"/> 角色名过长
		<enum name="CREATE_SHORTLEN" value="7" /> 角色名过短
		
		<variable name="newInfo" type="RoleInfo"/>	创建后的角色信息
		<variable name="error" type="int"/>	失败code
	</protocol>
	
	<!-- 进入服务器 -->
	<protocol  name="CEnterWorld" type="5" maxsize="32" prior="1">
		<variable name="roleid" type="long" />		ID
		<variable name="mac" type="string" />	mac地址
