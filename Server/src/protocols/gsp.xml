		<variable name="location1" type="int"/> 0没装
		<variable name="location2" type="int"/> 0没装
		<variable name="location3" type="int"/> 0没装
		<variable name="location4" type="int"/> 0没装
		<variable name="location5" type="int"/> 0没装
		
