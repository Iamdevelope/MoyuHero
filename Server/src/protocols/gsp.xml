		<enum name="AcceptTeam" 		value="2" />				接受组队
		<enum name="CheckEquipOn"		value="3" />				允许查看装备
		<enum name="AcceptGiven"		value="4" />				接受给予		
		<enum name="AcceptSmallGame"	value="5" />				接受小游戏邀请
		<enum name="EscapeConfirm" 		value="6" />				逃跑确认
		<enum name="PurchaseConfirm"	value="7" />				购买确认
